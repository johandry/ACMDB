<%= simple_form_for(@employee) do |f| %>
  <%= f.error_notification %>

  <div class="inputs">
    <%= f.input :name %>
    <%= f.input :first_name %>
    <%= f.input :last_name %>
    <%= f.input :email %>
    <%= f.association :team %>
  </div>

  <div class="actions">
    <%= f.button :submit %>
  </div>
<% end %>
